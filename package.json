    "@types/jest": "27.4.1",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "babel-jest": "27.5.1",
    "husky": "7.0.4",
    "immer": "^9.0.12",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "react": "18.0.0",
    "react-dom": "^18.0.0",
