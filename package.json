{
  "name": "react-remind",
  "version": "1.1.5",
  "description": "Remind is library for state management in React",
  "repository": "https://github.com/MichalTarasiuk/remind.git",
  "exports": {
    "import": "./dist/index.mjs",
    "default": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "browser": "./dist/index.umd.js"
  },
  "types": "./dist/src/remind.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "browser": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "author": {
    "name": "Michał Tarasiuk",
    "email": "tarasiuk.michal03@gmail.com"
  },
  "files": [
    "dist"
  ],
  "contributors": [
    "Michał Tarasiuk (https://github.com/MichalTarasiuk)"
  ],
  "workspaces": [
    ".",
    "examples/*"
  ],
  "keywords": [
    "react",
    "state",
    "manager",
    "management",
    "redux",
    "store"
  ],
  "bugs": {
    "url": "https://github.com/MichalTarasiuk/remind/issues"
  },
  "license": "MIT",
